@model ContactInfo

<footer class="grid-three-column footer">
    <div class="">
        <p>Links</p>
        <ul class="footer-links">
            <li><a href="/Home/Services">Services</a>
                <ul>
                    <li><a href="/Home/ResidentialServices">Residential Services</a></li>
                    <li><a href="/Home/CounselingServices">Counseling Services</a></li>
                </ul>    
            </li>
            <li><a href="/Home/PropertyManagement">Property Management</a></li>
            <li><a href="/Home/Testimonials">Testimonials</a></li>
            <li><a href="/Home/ContactUs">Contact Us</a></li>
            <li><a href="/Home/AboutUs">About Us</a></li>
            <li><a href="/Home/OurMission">Our Mission</a></li>
            <li><a href="/Home/Statistics">Statistics</a>
                <ul>
                    <li><a href="/Home/KeyStats">Key Statistics</a></li>
                    <li><a href="/Home/Costs">Costs</a></li>
                </ul>    
            </li>
        </ul>
        @* &copy; 2024 - @ViewData["SiteTitle"] - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a> *@
    </div>
    <div>
        @if(Model.SocialLinks != null) {
            <p>social media</p>
            <ul>
                @foreach (var item in Model.SocialLinks) {
                    <li><a href="@item.Url" class="@item.Name.ToLower()-blue"><i class="fa-brands fa-@item.Name.ToLower() @item.Name.ToLower()-blue"></i> @item.Name</a></li>
                }
            </ul>
        }
    </div>
    <div>
       <partial name="~/Views/Partials/_Footer_ContactUs.cshtml" model="Model" />
    </div>
</footer>